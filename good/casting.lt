
// Casting to bool
main 

    // Int casting (n <> 0)
    int n = 10;
    while (n)
        print(n);
        n = n - 1;
    endwhile


    // String casting (str <> "")
    string a = "A";
    string b = "";
    string c = "ccc";

    if (a) then 
        print(a); 
    endif

    if (b) then 
        print(b); 
    endif

    if (c) then 
        print(c); 
    endif

endmain 